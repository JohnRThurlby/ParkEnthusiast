{
  "name": "parkenthusiast",
  "version": "1.0.0",
  "description": "The Park Enthusiast",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "email-existence": "^0.1.6",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "fs": "^0.0.1-security",
    "google-map-react": "^1.0.5",
    "google-maps-react": "^2.0.2",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.3.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "node-ses": "^2.1.0",
    "nodemon": "^1.18.1",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react-addons": "^0.9.1-deprecated",
    "react-dropdown": "^1.5.0",
    "react-frame-component": "^4.0.0",
    "react-google-map": "^3.1.1",
    "react-pure-render": "^1.0.2",
    "react-tabs": "^2.2.2",
    "sequelize": "^4.38.0",
    "zipcodes": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnRThurlby/ParkEnthusiast.git"
  },
